<?xml version="1.0" encoding="UTF-8"?>
<!-- verinice Spring configuration file  -->
<!-- This file is referenced in contextConfigLocation parameter of web.xml -->
<!-- This is a configuration to delete orphan template relations. -->
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
  
  <bean id="deleteOrphanTemplateRelations" class="sernet.verinice.templates.DeleteOrphanTemplateRelations" init-method="init">
    <property name="elementDao" ref="cnaTreeElementDao" />
  </bean>
</beans>