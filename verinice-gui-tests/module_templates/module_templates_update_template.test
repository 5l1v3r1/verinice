log "module_templates_create"

create-it-verbund "IT-Verbund Template"

with [get-view Grundschutzmodell | get-tree] {
    select "  IT-Verbund Template/Anwendungen" | get-menu -path "Neue Anwendung" | click
    with [get-editor Anwendung | get-section "" | get-editbox -after [get-label Name]] {
        set-text "Anwendung-Template"
        key-type "M1+s"
    }
    select "  IT-Verbund Template/Anwendungen/A- Anwendung-Template" | get-menu -path "Modellierungs-Template" | click
    select "  IT-Verbund Template/Anwendungen/A- Anwendung-Template" | get-menu -path "Neuer benutzerdefinierter Baustein" | click
    with [get-editor "" | get-section "" | get-editbox -after [get-label Name]] {
        set-text "Baustein"
        key-type "M1+s"
    }
    select "  IT-Verbund Template/Anwendungen/A- Anwendung-Template/Baustein" | get-menu -path "Neue benutzerdefinierte Maßnahme" | click
    with [get-editor "" | get-section "" | get-editbox -after [get-label Name]] {
        set-text "Maßnahme Zentral"
        key-type "M1+s"
    }
    select "  IT-Verbund Template/Anwendungen/A- Anwendung-Template/Baustein" | get-menu -path "Neue benutzerdefinierte Maßnahme" | click
    with [get-editor "" | get-section "" | get-editbox -after [get-label Name]] {
        set-text "Maßnahme Normal"
        key-type "M1+s"
    }
    select "  IT-Verbund Template/Anwendungen/A- Anwendung-Template/Baustein/Maßnahme Zentral []" | get-menu -path "Zentral gepflegt" | click

    select "  IT-Verbund Template/Anwendungen/A- Anwendung-Template" | get-menu -path "Kopieren" | click
    create-it-verbund "IT-Verbund Modelliert"
    select "  IT-Verbund Modelliert/Anwendungen" | get-menu -path "Einfügen" | click

    select "  IT-Verbund Modelliert/Anwendungen/A- Anwendung-Template/Baustein/Maßnahme Zentral []" | get-menu -path "Öffnen" | click
    with [get-editor "Maßnahme Zentral []" | get-section ""] {
        get-editbox -after [get-label Name] | equals "Maßnahme Zentral" | verify-true
        get-editbox -after [get-label Beschreibung] | set-text "Maßnahme mit Template modelliert"
        key-type "M1+s"
    }
    select "  IT-Verbund Modelliert/Anwendungen/A- Anwendung-Template/Baustein/Maßnahme Normal []" | get-menu -path "Öffnen" | click
    with [get-editor "Maßnahme Normal []" | get-section ""] {
        get-editbox -after [get-label Name] | equals "Maßnahme Normal" | verify-true
        get-editbox -after [get-label Beschreibung] | set-text "Maßnahme ohne Template modelliert"
        key-type "M1+s"
    }

    select "  IT-Verbund Template/Anwendungen/A- Anwendung-Template/Baustein/Maßnahme Zentral []" | get-menu -path "Öffnen" | click
    with [get-editor "Maßnahme Zentral []" | get-section "" | get-editbox -after [get-label Beschreibung]] {
        equals "Maßnahme mit Template modelliert" | verify-true
    }
    select "  IT-Verbund Template/Anwendungen/A- Anwendung-Template/Baustein/Maßnahme Normal []" | get-menu -path "Öffnen" | click
    with [get-editor "Maßnahme Normal []" | get-section "" | get-editbox -after [get-label Beschreibung]] {
        equals "" | verify-true
    }
}

